{
  "name": "@jitaspace/esi-hooks",
  "description": "ESI React Hooks",
  "version": "0.1.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs,iife --dts --inject ./react-import.js",
    "clean": "rm -rf .turbo node_modules",
    "dev": "pnpm build --watch",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "type-check": "tsc --jsx react-jsx --noEmit",
    "prepublish": "pnpm build"
  },
  "dependencies": {
    "@react-hook/cache": "^1.1.1",
    "axios": "^1.5.0",
    "swr": "^2.2.2"
  },
  "devDependencies": {
    "@jitaspace/esi-client": "*",
    "@jitaspace/eslint-config": "*",
    "@jitaspace/prettier-config": "*",
    "@jitaspace/tsconfig": "*",
    "@types/node": "^18.17.18",
    "@types/react": "^18.2.22",
    "eslint": "^8.49.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": ">=18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@jitaspace/esi-client": "^0.2.2",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  }
}
