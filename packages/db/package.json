{
  "name": "@jitaspace/db",
  "description": "JitaSpace Database",
  "version": "0.1.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "db:studio": "prisma studio --browser none",
    "db:generate": "prisma generate",
    "db:generate:watch": "prisma:generate --watch",
    "db:push": "prisma db push --skip-generate",
    "db:migrate:deploy": "prisma migrate deploy",
    "dev": "concurrently -c \"auto\" --names \"Studio,Watch\" \"pnpm:db:studio\" \"pnpm:prisma:generate:watch\"",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "postinstall": "prisma generate",
    "type-check": "tsc --jsx react-jsx --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^5.3.0"
  },
  "devDependencies": {
    "@jitaspace/eslint-config": "*",
    "@jitaspace/prettier-config": "*",
    "@jitaspace/tsconfig": "*",
    "@types/node": "^18.17.16",
    "concurrently": "^8.2.1",
    "eslint": "^8.49.0",
    "prettier": "^3.0.3",
    "prisma": "^5.3.0",
    "typescript": "^5.2.2"
  }
}
