{
  "name": "@jitaspace/utils",
  "description": "Utilities for JitaSpace",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs,iife --dts",
    "clean": "rm -rf .turbo node_modules",
    "dev": "pnpm build --watch",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "type-check": "tsc --jsx react-jsx --noEmit",
    "prepublish": "pnpm build"
  },
  "devDependencies": {
    "@jitaspace/esi-client": "*",
    "@jitaspace/eslint-config": "*",
    "@jitaspace/prettier-config": "*",
    "@jitaspace/tsconfig": "*",
    "@types/node": "^18.17.19",
    "@types/react": "^18.2.22",
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
