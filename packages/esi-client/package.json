{
  "name": "@jitaspace/esi-client",
  "description": "ESI Client for JitaSpace",
  "version": "0.1.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "download-schema": "curl -L https://esi.evetech.net/latest/swagger.json | jq 'del(.paths.\"/route/{origin}/{destination}/\")' > swagger.json",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "kubb:generate": "AXIOS_BASE=https://esi.evetech.net/latest kubb",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@kubb/cli": "^1.11.5",
    "@kubb/core": "^1.11.3",
    "@kubb/swagger": "^1.11.1",
    "@kubb/swagger-client": "^1.11.1",
    "@kubb/swagger-tanstack-query": "^1.11.1",
    "@kubb/swagger-ts": "^1.11.1",
    "@kubb/swagger-zod": "^1.10.0",
    "@kubb/swagger-zodios": "^1.10.0",
    "@tanstack/react-query": "^4.36.1",
    "@zodios/core": "^10.9.6",
    "axios": "^1.5.1",
    "react": "18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jitaspace/eslint-config": "*",
    "@jitaspace/prettier-config": "*",
    "@jitaspace/tsconfig": "*",
    "@types/node": "^18.18.5",
    "@types/react": "^18.2.28",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
